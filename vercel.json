{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/dev/(.*)",
            "dest": "/dev/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ]
}