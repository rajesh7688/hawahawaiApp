{
    "redirects": [
      { "source": "/index.html", "destination": "/index.html" },
      { "source": "/site", "destination": "/public/site", "permanent": false },
      {
        "source": "/public",
        "destination": "https://github.com/vercel/vercel"
      },
      {
        "source": "/:path((?!uk/).*)",
        "has": [
          {
            "type": "header",
            "key": "x-vercel-ip-country",
            "value": "GB"
          }
        ],
        "destination": "/uk/:path*",
        "permanent": false
      }
    ]
  }